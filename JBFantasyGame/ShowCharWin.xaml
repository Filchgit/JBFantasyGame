<Window x:Class="JBFantasyGame.ShowCharWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JBFantasyGame"
        mc:Ignorable="d"
        Title="Character Sheet
        " Height="450" Width="800">
    <Grid>
        <TextBlock x:Name="ShowCharname" HorizontalAlignment="Left" Margin="60,26,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Height="51" Width="122" RenderTransformOrigin="0.012,-0.624"/>
        <TextBlock x:Name="ShowCharInt" HorizontalAlignment="Left" Margin="60,77,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Height="51" Width="122" RenderTransformOrigin="0.012,-0.624"/>
    </Grid>
</Window>
